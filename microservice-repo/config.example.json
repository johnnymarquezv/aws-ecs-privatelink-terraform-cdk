{
  "service": {
    "name": "microservice",
    "version": "1.0.0",
    "port": 8000,
    "log_level": "INFO",
    "enable_metrics": true,
    "rate_limit": 100
  },
  "consumer_services": [
    {
      "name": "user-service",
      "endpoint": "vpce-12345678-abcdefgh.vpce-svc-12345678.us-east-1.vpce.amazonaws.com",
      "port": 8080,
      "timeout": 30
    },
    {
      "name": "notification-service",
      "endpoint": "vpce-87654321-fedcba98.vpce-svc-87654321.us-east-1.vpce.amazonaws.com",
      "port": 8081,
      "timeout": 30
    }
  ],
  "monitoring": {
    "enable_health_checks": true,
    "enable_metrics": true,
    "enable_tracing": true
  },
  "security": {
    "enable_cors": true,
    "enable_rate_limiting": true,
    "trusted_hosts": ["*"]
  }
}
